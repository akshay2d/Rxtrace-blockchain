generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  schemas  = ["auth", "public"]
}

/// This model contains an expression index which requires additional setup for migrations. Visit https://pris.ly/d/expression-indexes for more info.
model companies {
  id                                String                @id @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  created_at                        DateTime?             @default(now()) @db.Timestamptz(6)
  company_name                      String
  contact_person                    String
  email                             String                @unique
  phone                             String
  address                           String
  gst_number                        String?               @unique
  gtin_prefix                       String?
  total_skus                        Int?
  industry                          String
  business_type                     String
  labels_per_month                  String?
  user_id                           String                @unique @db.Uuid
  subscription_plan                 String?               @default("free")
  labels_used                       Int?                  @default(0)
  labels_limit                      Int?                  @default(1000)
  paid_until                        DateTime?             @db.Timestamptz(6)
  subscription_status               String?
  trial_start_date                  DateTime?             @db.Timestamptz(6)
  trial_end_date                    DateTime?             @db.Timestamptz(6)
  extra_user_seats                  Int                   @default(0)
  extra_erp_integrations            Int                   @default(0)
  razorpay_customer_id              String?
  razorpay_subscription_id          String?
  razorpay_subscription_status      String?
  razorpay_plan_id                  String?
  subscription_cancel_at_period_end Boolean               @default(false)
  subscription_current_period_end   DateTime?             @db.Timestamptz(6)
  subscription_cancelled_at         DateTime?             @db.Timestamptz(6)
  subscription_updated_at           DateTime?             @db.Timestamptz(6)
  firm_type                         String?
  business_category                 String?
  contact_person_name               String?
  pan                               String?
  trial_activated_at                DateTime?             @db.Timestamp(6)
  erp_ingestion_mode                String?               @default("unit")
  unit_quota_balance                Int?                  @default(0)
  sscc_quota_balance                Int?                  @default(0)
  add_on_unit_balance               Int?                  @default(0)
  add_on_sscc_balance               Int?                  @default(0)
  last_quota_rollover_at            DateTime?             @db.Timestamptz(6)
  discount_type                     String?
  discount_value                    Decimal?              @db.Decimal(10, 2)
  discount_applies_to               String?               @default("both")
  discount_notes                    String?
  print_format                      String?
  printer_type                      String?
  printer_identifier                String?
  profile_completed                 Boolean               @default(false)
  updated_at                        DateTime?             @default(now()) @db.Timestamptz(6)
  gst                               String?
  razorpay_offer_id                 String?
  trial_started_at                  DateTime?             @db.Timestamptz(6)
  trial_ends_at                     DateTime?             @db.Timestamptz(6)
  trial_status                      String?
  audit_logs_archive                audit_logs_archive[]
  billing_usage                     billing_usage[]
  users                             users                 @relation(fields: [user_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  active_heads                      company_active_heads?
  company_add_ons                   company_add_ons[]
  company_discounts                 company_discounts[]
  credit_notes                      credit_notes[]
  generated_labels                  generated_labels[]
  handset_tokens                    handset_tokens[]
  handsets                          handsets[]
  labels_units                      labels_units[]
  packaging_rules                   packaging_rules[]
  packing_rules                     packing_rules[]
  quota_balances                    quota_balances[]
  refunds                           refunds[]
  scan_logs                         scan_logs[]
  seats                             seats[]
  support_requests                  support_requests[]
  usage_counters                    usage_counters[]
  usage_events                      usage_events[]

  @@index([gst_number], map: "idx_companies_gst")
  @@index([user_id], map: "idx_companies_user_id")
  @@schema("public")
}

model company_wallets {
  company_id   String   @id @db.Uuid
  balance      Decimal  @default(0) @db.Decimal(18, 2)
  credit_limit Decimal  @default(10000) @db.Decimal(18, 2)
  status       String   @default("ACTIVE")
  updated_at   DateTime @default(now()) @updatedAt @db.Timestamptz(6)

  @@index([company_id], map: "idx_wallets_company")
  @@schema("public")
}

model company_active_heads {
  company_id String    @id @db.Uuid
  heads      Json      @default("{}")
  updated_at DateTime  @default(now()) @updatedAt @db.Timestamptz(6)
  company    companies @relation(fields: [company_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@schema("public")
}

model billing_transactions {
  id            String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  company_id    String   @db.Uuid
  type          String
  subtype       String?
  count         Int      @default(1)
  amount        Float
  balance_after Float
  created_at    DateTime @default(now()) @db.Timestamptz(6)

  @@index([company_id], map: "idx_billing_transactions_company_id")
  @@schema("public")
}

model seats {
  id           String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  company_id   String    @db.Uuid
  user_id      String?   @db.Uuid
  email        String?
  role         String?   @default("operator")
  active       Boolean?  @default(false)
  status       String?   @default("pending")
  invited_at   DateTime? @db.Timestamptz(6)
  activated_at DateTime? @db.Timestamptz(6)
  created_at   DateTime? @default(now()) @db.Timestamptz(6)
  updated_at   DateTime? @default(now()) @updatedAt @db.Timestamptz(6)
  company      companies @relation(fields: [company_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
  users        users?    @relation(fields: [user_id], references: [id], onUpdate: NoAction)

  @@index([active], map: "idx_seats_active")
  @@index([company_id], map: "idx_seats_company_id")
  @@index([email], map: "idx_seats_email")
  @@index([status], map: "idx_seats_status")
  @@index([user_id], map: "idx_seats_user_id")
  @@schema("public")
}

model handset_tokens {
  id                String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  company_id        String    @db.Uuid
  token             String    @unique
  high_scan         Boolean?  @default(false)
  used              Boolean?  @default(false)
  created_at        DateTime? @default(now()) @db.Timestamptz(6)
  disabled          Boolean   @default(false)
  activated_at      DateTime? @db.Timestamp(6)
  activated_handset String?
  company           companies @relation(fields: [company_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@index([disabled])
  @@index([used])
  @@index([company_id], map: "idx_handset_tokens_company")
  @@schema("public")
}

model handsets {
  id                 String    @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  company_id         String    @db.Uuid
  seat_id            String?   @db.Uuid
  device_fingerprint String    @unique
  high_scan_enabled  Boolean?  @default(false)
  status             String?   @default("ACTIVE")
  activated_at       DateTime? @default(now()) @db.Timestamptz(6)
  company            companies @relation(fields: [company_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@index([company_id], map: "idx_handsets_company")
  @@schema("public")
}

model skus {
  id              String            @id @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  company_id      String            @db.Uuid
  sku_code        String            @db.VarChar(100)
  sku_name        String            @db.VarChar(255)
  created_at      DateTime?         @default(now()) @db.Timestamptz(6)
  updated_at      DateTime?         @db.Timestamptz(6)
  deleted_at      DateTime?         @db.Timestamptz(6)
  boxes           boxes[]
  labels_units    labels_units[]
  packaging_rules packaging_rules[]

  @@unique([company_id, sku_code])
  @@unique([company_id, sku_code], map: "skus_company_sku_code_unique")
  @@index([company_id])
  @@schema("public")
}

model packing_rules {
  id                           String     @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  sku_id                       String?
  version                      Int        @default(1)
  name                         String?
  strips_per_box               Int
  boxes_per_carton             Int
  cartons_per_pallet           Int
  allow_partial_last_container Boolean    @default(true)
  sscc_extension_digit         Int        @default(0) @db.SmallInt
  sscc_company_prefix          String
  sscc_sequence_key            String
  meta                         Json?      @default("{}")
  created_by                   String?    @db.Uuid
  created_at                   DateTime   @default(now()) @db.Timestamptz(6)
  company_id                   String?    @db.Uuid
  company                      companies? @relation(fields: [company_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@unique([company_id, sku_id, version], map: "packing_rules_company_sku_version_key")
  @@index([company_id], map: "idx_packing_rules_company_id")
  @@index([sku_id], map: "idx_packing_rules_sku")
  @@schema("public")
}

model sscc_counters {
  sequence_key String   @id
  last_serial  BigInt   @default(0)
  updated_at   DateTime @default(now()) @db.Timestamptz(6)

  @@schema("public")
}

enum TokenStatus {
  ACTIVE
  DISABLED

  @@map("tokenstatus")
  @@schema("public")
}


model Token {
  id              String      @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  userId          String      @map("userid")
  tokenNumber     String      @unique @map("tokennumber")
  generatedAt     DateTime    @default(now()) @db.Timestamptz(6) @map("generatedat")
  expiry          DateTime    @db.Timestamptz(6)
  status          TokenStatus @default(ACTIVE)
  activationCount Int         @default(0) @map("activationcount")
  maxActivations  Int         @default(10) @map("maxactivations")
  handsets        Handset[]

  @@map("token")
  @@index([userId], map: "idx_tokens_user_id")
  @@schema("public")
}


model Handset {
  id          String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  userId      String   @map("userid")
  deviceName  String   @map("devicename")
  tokenId     String   @map("tokenid")
  activatedAt DateTime @default(now()) @db.Timestamptz(6) @map("activatedat")
  active      Boolean  @default(true)

  token Token @relation(fields: [tokenId], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@map("handset")
  @@index([userId], map: "idx_handsets_user_id")
  @@index([tokenId], map: "idx_handsets_token_id")
  @@schema("public")
}

model pallets {
  id              String       @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  company_id      String       @db.Uuid
  sku_id          String?      @db.Uuid
  packing_rule_id String?      @db.Uuid
  sscc            String       @unique @db.VarChar(18)
  sscc_with_ai    String?
  meta            Json?        @default("{}")
  created_by      String?      @db.Uuid
  created_at      DateTime     @default(now()) @db.Timestamptz(6)
  sscc_level      String?
  parent_sscc     String?
  code            String?
  cartons         cartons[]
  strips_map      strips_map[]

  @@unique([company_id, sscc], map: "unique_sscc_per_company")
  @@index([company_id], map: "idx_pallets_company")
  @@index([sscc], map: "idx_pallets_sscc")
  @@schema("public")
}

model cartons {
  id           String       @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  company_id   String       @db.Uuid
  pallet_id    String?      @db.Uuid
  sku_id       String?      @db.Uuid
  code         String?
  meta         Json?        @default("{}")
  created_by   String?      @db.Uuid
  created_at   DateTime     @default(now()) @db.Timestamptz(6)
  sscc_level   String?
  parent_sscc  String?
  sscc         String?      @db.VarChar(18)
  sscc_with_ai String?
  boxes        boxes[]
  pallet       pallets?     @relation(fields: [pallet_id], references: [id], onUpdate: NoAction)
  strips_map   strips_map[]

  @@index([pallet_id], map: "idx_cartons_pallet")
  @@schema("public")
}

